<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <services>

      <!--********************************** Calc Service ********************************** -->
      <service name="Velib_Service_Subscrib.SubscriberService" behaviorConfiguration="CalcServiceBehavior">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:9011/SubService"/>
          </baseAddresses>
        </host>

        <endpoint address="" binding="wsDualHttpBinding" contract="Velib_Service_Subscrib.ISubscriberService"/>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
      </service>
    </services>

    <!-- ********************************** behaviors ********************************** -->
    <behaviors>
      <serviceBehaviors>
        <!-- CalcService Behavior -->
        <behavior name="CalcServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="true "/>
        </behavior>
      </serviceBehaviors>
    </behaviors>

  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
</configuration>
